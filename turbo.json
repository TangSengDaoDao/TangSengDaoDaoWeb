{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev-web": {
      "cache": false
    },
    "dev-ele": {
      "cache": false
    },
    "build-web": {
      "outputs": ["build/**"]
    },
    "build-web:rsdoctor": {
      "outputs": ["build/**"]
    },
    "build-ele": {
      "outputs": ["dist-ele/**"],
      "dependsOn": ["^build-web"]
    },
    "build-ele:mac": {
      "outputs": ["dist-ele/**"],
      "dependsOn": ["^build-web"]
    },
    "build-ele:win": {
      "outputs": ["dist-ele/**"],
      "dependsOn": ["^build-web"]
    },
    "build-ele:linux": {
      "outputs": ["dist-ele/**"],
      "dependsOn": ["^build-web"]
    },
    "build-ele:linux-arm64": {
      "outputs": ["dist-ele/**"],
      "dependsOn": ["^build-web"]
    },
    "preview": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "dependsOn": ["^clean"]
    }
  },
  "ui": "stream"
}

{
  "name": "@tsdaodao/web",
  "version": "1.0.6",
  "private": true,
  "main": "out-election/main/index.js",
  "homepage": ".",
  "author": {
    "name": "budou",
    "email": "1769794040@qq.com"
  },
  "scripts": {
    "dev-web": "rsbuild dev",
    "dev-ele": "concurrently -k -n=web,ele -c=green,blue \"yarn dev-web\" \"wait-on tcp:127.0.0.1:3000 && npm-run-all watch\"",
    "watch": "tsc-watch -p tsconfig.e.json --onSuccess \"npm-run-all start:electron\"",
    "start:electron": "cross-env NODE_ENV=development electron .",
    "build-web": "rsbuild build",
    "build-web:rsdoctor": "cross-env RSDOCTOR=true rsbuild build",
    "build-ele": "tsc -p tsconfig.e.json && electron-builder -mwl -c",
    "build-ele:mac": "tsc -p tsconfig.e.json && electron-builder --mac -c",
    "build-ele:win": "tsc -p tsconfig.e.json && electron-builder --win -c",
    "build-ele:linux": "tsc -p tsconfig.e.json && electron-builder --linux --x64 -c",
    "build-ele:linux-arm64": "tsc -p tsconfig.e.json && electron-builder --linux --arm64 -c",
    "preview": "cross-env PREVIEW=true rsbuild build && rsbuild preview",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "clean": "rimraf node_modules out-election build dist-ele .turbo"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.1.0",
    "@tsdaodao/base": "*",
    "@tsdaodao/contacts": "*",
    "@tsdaodao/datasource": "*",
    "@tsdaodao/login": "*",
    "@types/react-lazyload": "^3.2.0",
    "@types/react-mentions": "^4.1.5",
    "@types/react-virtualized": "^9.21.22",
    "classnames": "^2.3.1",
    "electron-log": "^5.1.1",
    "electron-screenshots": "^0.5.26",
    "howler": "^2.2.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-app-rewired": "^2.1.8",
    "react-lazyload": "^3.2.0",
    "react-mentions": "^4.3.1",
    "react-virtualized": "^9.22.5",
    "react-virtuoso": "^4.5.0",
    "tmp": "^0.2.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.0.4",
    "@rsbuild/plugin-react": "^1.0.2",
    "@rsbuild/plugin-sass": "^1.0.1",
    "@rsbuild/plugin-svgr": "^1.0.2",
    "@rsdoctor/rspack-plugin": "^0.4.2",
    "@tauri-apps/cli": "1.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/howler": "^2.2.11",
    "@types/jest": "^24.0.0",
    "@types/node": "^20.10.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "electron": "26.0.0",
    "electron-builder": "^24.9.1",
    "eslint-config-react-app": "^7.0.1",
    "kill-port": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-normalize": "^10.0.1",
    "react-scripts": "5.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "tsc-watch": "^6.0.4",
    "typescript": "^4.5.5",
    "wait-on": "^7.2.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}

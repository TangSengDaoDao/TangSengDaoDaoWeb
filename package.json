{
  "name": "tsdd",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "resolutions": {
    "react-error-overlay": "6.0.11"
  },
  "scripts": {
    "dev-web": "turbo run dev-web",
    "dev-ele": "turbo run dev-ele",
    "build-web": "turbo run build-web",
    "build-web:rsdoctor": "turbo run build-web:rsdoctor",
    "build-ele": "turbo run build-ele",
    "build-ele:mac": "turbo run build-web && turbo run build-ele:mac",
    "build-ele:win": "turbo run build-web && turbo run build-ele:win",
    "build-ele:linux": "turbo run build-web && turbo run build-ele:linux",
    "build-ele:linux-arm64": "turbo run build-web && turbo run build-ele:linux-arm64",
    "preview": "turbo run preview",
    "bootstrap": "yarn install",
    "clean": "turbo run clean && rimraf node_modules .turbo",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "rimraf": "^5.0.5",
    "turbo": "2.4.4"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.19"
  }
}
